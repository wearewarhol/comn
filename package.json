    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@material-ui/styles": "^4.11.4",
    "@warhol/tsconfig": "2.0.0",
    "chroma-js": "^2.1.2",
    "classnames": "^2.3.1",
    "lottie-web": "^5.7.8",
    "prismjs": "^1.23.0",
    "react-lazyload": "^3.2.0",
    "react-router-dom": "^5.2.0",
    "react-simple-code-editor": "^0.11.0",
    "react-transition-group": "^4.4.1",
    "showdown": "^1.9.1",
    "walls": "^2.3.0"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.3",
