    "@types/showdown": "^1.9.3",
    "copyfiles": "^2.4.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.27.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "jest-enzyme": "^7.1.2",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
