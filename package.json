{
  "name": "@warhol/comn",
  "version": "1.3.3",
  "description": "Commons Library of React Components",
  "main": "build/index.js",
  "module": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "https://github.com/wearewarhol/comn.git",
  "author": "Hans Christian Reinl <info@drublic.de>",
  "license": "MIT",
  "scripts": {
    "prepare": "yarn build",
    "lint": "eslint ./src/ --ext ts,tsx",
    "copy:css": "copyfiles -u 1 './src/**/*.{css,svg}' build",
    "copy:json": "copyfiles -u 1 './src/**/*.json' build",
    "ts:build": "tsc -p tsconfig.json",
    "start:dev": "yarn ts:build --watch",
    "build": "yarn copy:css && yarn copy:json && yarn ts:build"
  },
  "peerDependencies": {
    "@material-ui/styles": "^4.8.2",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "dependencies": {
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "lottie-web": "^5.6.0",
    "prettier": "^1.19.1",
    "prismjs": "^1.18.0",
    "react-simple-code-editor": "^0.10.0",
    "walls": "^2.0.7"
  },
  "devDependencies": {
    "@types/chroma-js": "^1.4.3",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.4",
    "@types/jest": "^24.0.25",
    "@types/prismjs": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "copyfiles": "^2.1.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^24.9.0",
    "jest-canvas-mock": "^2.2.0",
    "jest-enzyme": "^7.1.2",
    "typescript": "~3.7.4"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "jest": {
    "watchPathIgnorePatterns": [
      "node_modules",
      "build"
    ]
  }
}
